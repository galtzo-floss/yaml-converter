module Yaml
  module Converter
    VERSION: String

    class Error < ::StandardError; end
    class InvalidArgumentsError < Error; end
    class RendererUnavailableError < Error; end
    class PandocNotFoundError < Error; end

    def self.to_markdown: (String yaml_string, ?options: Hash[Symbol, untyped]) -> String
    def self.validate: (String yaml_string) -> { status: Symbol, error: Exception? }
    def self.convert: (input_path: String, output_path: String, ?options: Hash[Symbol, untyped]) -> { status: Symbol, output_path: String, validation: { status: Symbol, error: Exception? } }
  end
end
